#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int dup = n;
    int sum = 0;
    while(n>0){
    int ld = n %10;
    int cube = pow(ld,3);
    //int cube = ld * ld * ld;
    sum = sum + cube;
    n = n/ 10;
    }
    if(sum ==  dup){
        cout<< dup <<"   is armstrong number";
    }
    else
        cout<<"not a armstrong number";
    
}
