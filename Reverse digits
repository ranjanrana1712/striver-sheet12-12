#include<bits/stdc++.h>
using namespace std;
//to count the digits

int main(){
    int n;
    cin>>n;
    int count=0;
    
    int revNum=0;
    //123  into 321
    while(n>0){
    int ld;
    ld = n % 10;
    
    revNum = (revNum * 10) + ld;
    
    n = n/10;
        
    }
    cout<<revNum;
}

