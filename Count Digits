#include<bits/stdc++.h>
using namespace std;
//to count the digits

int main(){
    int n;
    cin>>n;
    int count=0;
    int temp;
    
    while(n>0){
        //the number of times it didived is equal to number of digits
        n = n/10;
        count = count + 1;
        
    }
    cout<< count;
    
}

************approach 2 using log function*****************

#include<bits/stdc++.h>
using namespace std;
//to count the digits

int main(){
    int n;
    cin>>n;
    int count=0;
    int temp;
    //why int : to truncate
    temp = (int) (log10(n)+1);
    cout<< temp;
}

//time complexity is o (log(n))
